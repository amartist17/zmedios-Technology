<script>
   $(document).ready(function() {
    $('#helpform').submit(function(e) {
        e.preventDefault(); // Prevent the default form submission

        var formData = $(this).serialize(); // Serialize the form data
        var $submitButton = $('#submitbtn'); // Cache the submit button by ID

        $.ajax({
            type: "POST",
            url: "/needhelp",
            data: formData,
            success: function(response) {
                // Change the button color to green and text to 'Message Sent'
                $submitButton.css('background-color', '#28a745'); // Green color
                $submitButton.text('Message Sent'); // Update text

                // Clear the form fields
                $('#helpform').trigger('reset');

                // After 5 seconds, revert the button color and text
                setTimeout(function() {
                    $submitButton.css('background-color', ''); // Revert to original color
                    $submitButton.text('Submit Now'); // Revert text
                }, 5000); // 5 seconds delay
            },
            error: function(xhr, status, error) {
                // Change the button color to red and text to 'An error occurred'
                $submitButton.css('background-color', '#dc3545'); // Red color
                $submitButton.text('An error occurred'); // Update text

                // After 5 seconds, revert the button color and text
                setTimeout(function() {
                    $submitButton.css('background-color', ''); // Revert to original color
                    $submitButton.text('Submit Now'); // Revert text
                }, 5000); // 5 seconds delay
            }
        });
    });
});

</script>



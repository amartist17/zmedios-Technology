<style>
    /* Base alert styling */
.alert {
    padding: 10px;
    margin-bottom: 20px;
    border: 1px solid transparent;
    border-radius: 4px;
    width: 90%;
    margin-left: auto;
    margin-right: auto;
    box-shadow: 0 2px 4px rgba(0,0,0,.1);
    animation: fadeInOut 4s forwards;
}

/* Success alert styling */
.alert-success {
    background-color: #28a745;
    color: white;
    border-color: #28a745;
}

/* Error alert styling */
.alert-danger {
    background-color: #dc3545;
    color: white;
    border-color: #dc3545;
}

/* Animation for the alert (fade in and then fade out) */
@keyframes fadeInOut {
    0% {
        opacity: 0;
        transform: translateY(-20px);
    }
    10% {
        opacity: 1;
        transform: translateY(0);
    }
    90% {
        opacity: 1;
    }
    100% {
        opacity: 0;
        transform: translateY(-20px);
    }
}

</style>

